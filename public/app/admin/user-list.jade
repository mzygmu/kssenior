.content.base-panel
  table.table
    tr(ng-repeat="user in users")
      td {{user.firstName + ' ' + user.lastName}}
      td {{user.username}}
      td {{user.roles}}
      td
        button.btn.btn-warning(ng-click='setAdmin(user)') set Admin  
      td
        button.btn.btn-warning(ng-click='removeUser(user)') Usun