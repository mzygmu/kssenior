body
  background-image: url("../img/bg.png");
  font: 15px 'Enriqueta', serif;
  line-height: 1.5em;

.navbar-header a.navbar-brand
  margin-top -15px
  margin-bottom -20px
  h6
    color #ccc

.navbar {
  margin-bottom: 0px;
}
.navbar-inverse {
  background-color: rgb(27, 27, 27);
  border-color: rgb(27, 27, 27);
}

.div-box {
  position: absolute;
  background-color: rgba(0, 0, 0, 0.3);
  padding: 15px 15px;
  margin: 8px 8px;
  font-weight: 300;
  color: #FFFFFF;
}

.content {
  width: 1024px;
  margin: 0 auto;
  margin-bottom: 15px;
  position: relative;
}
.content.main {
  width: 1040px;
  padding-top: 0px;
  padding-bottom: 0px;
  margin-left: -8px;
}

.main-elem {
  list-style: none; 
  width: 330px;
  height: 411px;
  padding: 0;  
  display: inline-block;
  vertical-align: top;
}
.main-elem-text {
  vertical-align: middle;
}
.main-elem.shadow {
  border: 15px solid rgb(27, 27, 27);
  border-image-source: initial;
  border-image-slice: initial;
  border-image-width: initial;
  border-image-outset: initial;
  border-image-repeat: initial;
  position: relative;
  margin-right: 8px;
  margin-left: 8px;
  top: 0px;
  bottom: 0px;
  left: 0px;
  right: 0px;
  background-color: rgb(27, 27, 27);
  border-radius: 0px;
  box-shadow: rgba(0, 0, 0, 0.6) 0px 1px 4px 0px;
}
div.text-box.main-elem-text {
  font-size: 33px;
  visibility: visible;
  left: 0px;
  top: 240px;
  width: 200px;
  height: 55px;
  position: absolute;
  z-index: auto;
}

.text-box {
  border: 5px solid rgb(27, 27, 27);
  margin-right: 2px;
  margin-left: 0px;
  top: 0px;
  bottom: 0px;
  left: 0px;
  right: 0px;
  background-color: rgb(27, 27, 27);
  border-radius: 0px;
  box-shadow: rgba(0, 0, 0, 0.6) 0px 1px 4px 0px;
  color: #fff;
}
#logo-text-1 {
  font-size: 33px;
  visibility: visible;
  left: 25px;
  top: 25px;
  width: 320px;
  height: 55px;
  position: absolute;
  z-index: auto;
}
#logo-text-2 {
  font-size: 33px;
  visibility: visible;
  left: 115px;
  top: 115px;
  width: 230px;
  height: 55px;
  position: absolute;
  z-index: auto;
}
#logo-text-3 {
  font-size: 33px;
  visibility: visible;
  left: 40px;
  top: 330px;
  width: 480px;
  height: 55px;
  position: absolute;
  z-index: auto;
}
#logo-button {
  border: 2px solid rgb(51, 100, 175);
  transition: border-color 0.4s ease 0s, background-color 0.4s ease 0s;
  -webkit-transition: border-color 0.4s ease 0s, background-color 0.4s ease 0s;
  border-radius: 5px;

  font-size: 22px;
  visibility: visible;
  right: 40px;
  top: 330px;
  width: 220px;
  height: 55px;
  position: absolute;
  z-index: auto;
}

a {
  color: #fff;
}

.pic-desc {
  text-align: left;
  vertical-align: top;
  display: inline-block;
  padding-top: 0px;
  padding-bottom: 0px;
  padding-right: 15px;
  padding-left: 0px;
}
.pic-desc.desc {
  width: 649px;
}
.pic-desc h4 {
  color: rgb(51, 100, 175); 
  font: 25px/1.2em 'Enriqueta', serif; 
}
.pic-desc h2 {
  color: rgb(186, 40, 0);
  font: 40px/1.1em 'Enriqueta', serif;
}

.senior-table-bg {
  width: 994px;
  text-align: left;
  padding-left: 15px;
  border: 5px solid rgb(27, 27, 27);
  margin-right: 2px;
  margin-left: 0px;
  margin-top: 15px;
  top: 0px;
  bottom: 0px;
  left: 0px;
  right: 0px;
  background-color: rgb(27, 27, 27);
  border-radius: 0px;
  box-shadow: rgba(0, 0, 0, 0.6) 0px 1px 4px 0px;
}

div.news {
  width: 994px;
  text-align: left;
  padding-left: 15px;
  border: 5px solid rgb(27, 27, 27);
  margin-right: 2px;
  margin-left: 0px;
  margin-top: 15px;
  top: 0px;
  bottom: 0px;
  left: 0px;
  right: 0px;
  background-color: rgb(27, 27, 27);
  border-radius: 0px;
  box-shadow: rgba(0, 0, 0, 0.6) 0px 1px 4px 0px;
}
div.news div {
  margin-right: 5px;
